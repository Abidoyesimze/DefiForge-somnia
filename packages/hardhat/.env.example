# Hardhat Environment Variables
# Copy this file to .env and fill in your actual values

# Private Keys (REQUIRED)
SOMNIA_PRIVATE_KEY=your_somnia_private_key_here
ETN_PRIVATE_KEY=your_etn_private_key_here

# API Keys (OPTIONAL - only needed for specific features)
ALCHEMY_API_KEY=your_alchemy_api_key_here
ETHERSCAN_MAINNET_API_KEY=your_etherscan_api_key_here
ETHERSCAN_OPTIMISTIC_API_KEY=your_optimism_etherscan_api_key_here
BASESCAN_API_KEY=your_basescan_api_key_here

# Runtime Deployer (OPTIONAL - for mainnet deployments)
__RUNTIME_DEPLOYER_PRIVATE_KEY=your_mainnet_deployer_private_key_here

# Feature Flags
MAINNET_FORKING_ENABLED=false
CI=false
